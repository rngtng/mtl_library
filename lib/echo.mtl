;;

// echo a integer list as string
fun ILecho list=
  Secho listtostr list
;;

fun ILecholn list=
  ILecho list;
  Secholn ""
;;

// echo a tuple list as string
fun TLecho tlist=
  Secho TLtoS tlist
;;
